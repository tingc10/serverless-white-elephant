{
  "name": "white-elephant",
  "version": "1.0.0",
  "description": "Serverless app that runs the server code for white elephant gift exchange",
  "main": "index.js",
  "repository": "https://github.com/tingc10/serverless-white-elephant",
  "author": "Ting Chen <tingshen.chen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "format": "yarn lint --fix",
    "build": "tsc --noEmit",
    "start": "sls offline start",
    "deploy": "sls deploy",
    "deploy:client": "sls client deploy --no-confirm",
    "dynamodb:start": "sls dynamodb start",
    "dynamodb:build": "tsc --outDir ./tests/build ./tests/*",
    "dynamodb:seed": "yarn dynamodb:build; node ./tests/build/seed-local-dynamodb.js",
    "dynamodb:migrate": "yarn dynamodb:build; node ./tests/build/migrate-user-rooms.js",
    "dynamodb:install": "sls dynamodb install"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.18.2",
    "aws-lambda": "^1.0.6",
    "aws-lambda-graphql": "^1.0.0-alpha.19",
    "aws-sdk": "^2.775.0",
    "class-validator": "^0.12.2",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.1.0",
    "nanoid": "^3.1.16",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/aws-lambda": "^8.10.64",
    "@types/aws-sdk": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-jest": "^26.6.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-module-resolver": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "serverless": "^2.11.1",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-finch": "^2.6.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-offline-dynamodb-stream": "^1.0.19",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-webpack": "^5.3.5",
    "ts-loader": "^8.0.6",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.1.3",
    "webpack-node-externals": "^2.5.2"
  }
}
